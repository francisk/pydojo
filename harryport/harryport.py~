from functools import partial

DISCOUNT={5:0.75,
          4:0.8,
          3:0.85,
          2:0.9,
          1:1}
BOOK_PRICE=8

def get_total_price(book_price,discount,buying_strategy):
    pass


def get_distinct_strategies(book_count,book_combinations):
    for choice in range(1,book_combinations+1):
        if book_count/choice > 0:
            if (book_count-choice)>0:
                for result in get_distinct_strategies(book_count-choice,book_combinations):
                    yield (choice,)+ result
            else:
                yield (choice,)
        else:
            continue


def get_min_price(book_group,price_stratege):
    pass


def smoke():
    print list(get_distinct_strategies(3,5))


if __name__ == '__main__':
    smoke()

